{
  "name": "module5-ai-integration-agent",
  "version": "1.0.0",
  "description": "AI 서비스 통합 전담 AI 에이전트 모듈",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts --fix",
    "type-check": "tsc --noEmit",
    "ai:test": "tsx src/tests/ai-integration.test.ts",
    "ai:benchmark": "tsx src/benchmarks/performance.ts",
    "ai:monitor": "tsx src/monitoring/ai-metrics.ts"
  },
  "dependencies": {
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "langchain": "^0.0.190",
    "llamaindex": "^0.1.0",
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "zod": "^3.20.2",
    "tiktoken": "^1.0.7",
    "winston": "^3.8.2",
    "redis": "^4.6.5",
    "bull": "^4.10.4"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "typescript": "^4.9.5",
    "tsx": "^3.12.3",
    "jest": "^29.4.3",
    "@types/jest": "^29.4.0",
    "nock": "^13.3.0"
  },
  "keywords": [
    "ai",
    "openai",
    "langchain",
    "nlp",
    "machine-learning",
    "automation",
    "ai-agent"
  ],
  "author": "Module5 AI Integration Agent",
  "license": "MIT",
  "agent_config": {
    "type": "ai_specialist",
    "auto_start": true,
    "workspace": "modules/module5",
    "responsibilities": [
      "AI API 통합",
      "자연어 처리",
      "AI 워크플로우",
      "모델 최적화"
    ]
  }
}