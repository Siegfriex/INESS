{
  "name": "module3-backend-agent",
  "version": "1.0.0",
  "description": "백엔드 API 개발 전담 AI 에이전트 모듈",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts --fix",
    "type-check": "tsc --noEmit",
    "functions:dev": "firebase emulators:start --only functions",
    "functions:deploy": "firebase deploy --only functions"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^4.2.0",
    "helmet": "^6.0.1",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "rate-limiter-flexible": "^2.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "firebase-functions-test": "^3.0.0",
    "jest": "^29.4.3",
    "supertest": "^6.3.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "backend",
    "api",
    "firebase",
    "nodejs",
    "typescript",
    "ai-agent"
  ],
  "author": "Module3 Backend Agent",
  "license": "MIT",
  "agent_config": {
    "type": "backend_developer",
    "auto_start": true,
    "workspace": "modules/module3",
    "responsibilities": [
      "API 개발",
      "백엔드 로직",
      "데이터베이스 연동",
      "보안 및 인증"
    ]
  }
}
