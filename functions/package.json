{
  "name": "maumlog-functions",
  "version": "2.2.1",
  "description": "Firebase Cloud Functions for MaumLog V2.2",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint": "eslint --ext .ts,.js src/",
    "lint:fix": "eslint --ext .ts,.js src/ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "gcp:init": "node -e \"require('./lib/config/gcpServices.js').gcpServiceManager.initializeGCPInfrastructure()\""
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/bigquery": "^6.2.0",
    "@google-cloud/kms": "^3.1.0",
    "@google-cloud/logging": "^10.5.0",
    "@google-cloud/monitoring": "^3.0.0",
    "@google-cloud/pubsub": "^3.7.1",
    "@google-cloud/secret-manager": "^4.2.2",
    "@google-cloud/speech": "^5.5.0",
    "@google-cloud/storage": "^6.10.1",
    "@google-cloud/text-to-speech": "^4.2.3",
    "@google-cloud/translate": "^8.0.0",
    "@notionhq/client": "^4.0.1",
    "@sendgrid/mail": "^7.7.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "openai": "^4.0.0",
    "stripe": "^12.9.0",
    "twilio": "^4.11.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-plugin-import": "^2.27.5",
    "firebase-functions-test": "^3.1.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
